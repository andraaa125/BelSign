<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="507.0" prefWidth="642.0" spacing="10.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.belsign.gui.controllers.UserListViewController">
    <children>
        <Label style="-fx-font-size: 20; -fx-font-weight: bold;" text="Users" />
        <TableView fx:id="userTable" prefHeight="400.0" prefWidth="600.0">
            <columns>
                <TableColumn fx:id="colFirstName" text="First Name" />
                <TableColumn fx:id="colLastName" text="Last Name" />
                <TableColumn fx:id="colUsername" text="Username" />
            </columns>
        </TableView>
      <Button fx:id="backBtn" mnemonicParsing="false" onAction="#onClickBack" prefHeight="26.0" prefWidth="52.0" text="Back" />
    </children>
    <padding>
        <Insets bottom="20" left="20" right="20" top="20" />
    </padding>
</VBox>
